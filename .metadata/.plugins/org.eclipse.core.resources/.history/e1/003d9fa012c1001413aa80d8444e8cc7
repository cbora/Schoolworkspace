#ifndef EDGE_H_
#define EDGE_H_
#include <string>
#include <vector>
#include "Edge.h"
#include "Vertex.h"

using namespace std;
class Vertex;

class Edge{
private:
	char weight;
public:
	Vertex vertexTo;
	//CONSTRUCTOR
	//Edge(Vertex *vertexTo, char weight);
	Edge(Vertex vertexTo, char weight);

	//GETTER METHODS maybe a set
	Vertex returnToVertex(){
		return vertexTo;
	}
	char returnWeight(){
		return weight;
	}
	void makeFinalFalse();

};




#endif /* EDGE_H_ */
