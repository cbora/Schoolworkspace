<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="neutronwombat.injectionprototype" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddContactActivity"
            android:label="@string/title_activity_add_contact"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="neutronwombat.injectionprototype.MainActivity" />
        </activity>
        <activity
            android:name=".ViewContactsActivity"
            android:label="@string/title_activity_view_contacts" >
        </activity>
        <activity
            android:name=".SelectOptionActivity"
            android:label="@string/title_activity_select_option"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="neutronwombat.injectionprototype.MainActivity" />
        </activity>
    </application>

</manifest>
