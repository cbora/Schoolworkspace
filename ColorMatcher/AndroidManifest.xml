<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fivedirections.colormatcher"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ColorMatcherActivity"
            android:label="@string/title_activity_color_matcher"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name="TitleScreenActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ScoreDisplayActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="TimeSelectActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>